#include <iostream>
using namespace std ;
int main(void){
    int n;
    cin>>n;
    int s[n],b[n],time=n;
    for(int i=0;i<n;i++){
        cin>>s[i]>>b[i];
        //cout<<"s:"<<s[i]<<" b:"<<b[i]<<endl;
    }
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            if(s[i]==b[j]){
                time-=1;break;
                }
        }
   
    } cout<<time<<endl;
return 0;
}
